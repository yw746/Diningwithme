<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".StratActivity" >







    <Spinner
        android:id="@+id/number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        style="@android:style/Widget.Spinner"
        android:layout_below="@+id/price"
        android:layout_alignParentRight="true" />



    <Spinner
        android:id="@+id/price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        style="@android:style/Widget.Spinner"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/number" />

    <Button
        android:id="@+id/submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="15dp"
        android:text="Submit" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find your location"
        android:id="@+id/location"
        android:layout_below="@+id/number"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/number" />

    <DatePicker
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/today"
        android:layout_below="@+id/location"
        android:layout_alignParentLeft="true"
        android:layout_above="@+id/submit"
        android:calendarViewShown="false"
        android:scaleX=".5"
        android:scaleY=".5"/>

    <TimePicker
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/now"
        android:layout_alignTop="@+id/today"
        android:layout_alignParentRight="true"
        android:layout_above="@+id/submit"
        android:scaleX=".5"
        android:scaleY=".5"/>


</RelativeLayout><!-- From: file:/D:/Users/android%20studio%20workspace/Diningwithme/app/src/main/res/layout/activity_start.xml -->